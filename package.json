{
  "name": "node-goodreads",
  "description": "Wrapper for the Goodreads API",
  "version": "0.1.1",
  "author": "Huey <leejinhuey@gmail.com>",
  "scripts": {
    "build": "babel . --out-dir dist --ignore node_modules,dist",
    "prepublish": "npm run build",
    "start": "node node_modules/.bin/babel-node -x .ts examples/example"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hueyy/node-goodreads"
  },
  "keywords": [
    "goodreads",
    "books",
    "lists"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "oauth": "^0.9.15",
    "query-string": "^6.13.1",
    "xml2js": ">= 0.1.9"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "babel-preset-env": "",
    "inquirer": "^7.2.0",
    "tsc": "^1.20150623.0",
    "typescript-eslint-parser": "^22.0.0"
  },
  "directories": {
    "lib": "dist/lib/goodreads",
    "example": "dist/examples"
  },
  "main": "dist/index.js",
  "engines": {
    "node": ">= 0.2.0"
  }
}
